<h2>New book</h2>
<form class="new_book" id="new_book" action="/books" accept-charset="UTF-8" method="post">
  <input name="utf8" type="hidden" value="✔︎"></input>
  <input type="hidden" name="authenticity_token" value=""></input>
  <div class="field">
    <label for="book_title">Title</label>
    <input class="book_title" type="text" name="book[title]" id="book_title"></input>
  </div>
  
  <div class="field">
    <label for="book_body">Body</label>
    <textarea class="book_body" name="book[body]" id="book_body"></textarea>
  </div>  
  
  <div class="actions">
    <input type="submit" name="commit" value="Create book" data-disable-with="Create Book"></input>
  </div>
</form>